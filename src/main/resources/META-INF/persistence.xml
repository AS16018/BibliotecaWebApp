    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.TipoFinalizacionDeReserva</class>
    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.MultasUsuarios</class>
    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.Reservas</class>
    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.DireccionUsuarios</class>
    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.Usuarios</class>
    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.EstadoUsuarios</class>
    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.RolesUsuarios</class>
    <class>ues.occ.edu.sv.tpi135.bibliotecawebapp.entity.RolesUsuario</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
